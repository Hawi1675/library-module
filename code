import math

while True:
    try:
        num = float(input("Enter a positive number: "))
        if num > 0:
            break
        print("Please enter a positive number.")
    except:
        print("Invalid input. Try again.")

print(f"\nSquare root: {math.sqrt(num):.2f}")
print(f"Natural log: {math.log(num):.2f}")
print(f"Sine: {math.sin(num):.2f}")

print(f"\nPi: {math.pi:.5f}")
print(f"Euler's number: {math.e:.5f}")
    

